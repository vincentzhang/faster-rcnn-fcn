train_net: "models/ins/VGG16/detect_end2end/train.prototxt"
base_lr: 1e-10
#0.001
lr_policy: "step"
gamma: 0.1
stepsize: 50000
display: 1
average_loss: 100
momentum: 0.9
weight_decay: 0.0005
# We disable standard caffe solver snapshotting and implement our own snapshot
# function
snapshot: 0
# We still use the snapshot prefix, though
snapshot_prefix: "vgg16_detect_ins_fg10" # _m for actual mean, 300 for scale
iter_size: 2
